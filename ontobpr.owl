    <!-- https://w3id.org/ontobpr#afterActivity -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#afterActivity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/ontobpr#Activity"/>
        <rdfs:comment>an activity is executed after another activity</rdfs:comment>
        <rdfs:label>after activity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#applies -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#applies">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/obpa#BuildingApplication"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#BuildingApplication"/>
        <rdfs:range rdf:resource="https://w3id.org/isoprops#Dictionary"/>
        <rdfs:range rdf:resource="https://w3id.org/ontobpr#BuildingCodeDictionary"/>
        <rdfs:comment>BuildingApplication applies a BuildingCodeDictionary for checking</rdfs:comment>
        <rdfs:label>applies</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#belongsToActivity -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#belongsToActivity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#ReviewStatement"/>
        <rdfs:range rdf:resource="https://w3id.org/ontobpr#Activity"/>
        <rdfs:comment>a ReviewStatement belongs to a particular Activity</rdfs:comment>
        <rdfs:label>belongs to activity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#checks -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#checks">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/obpa#BuildingAuthority"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#BuildingAuthority"/>
        <rdfs:range rdf:resource="https://w3id.org/isoprops#Dictionary"/>
        <rdfs:range rdf:resource="https://w3id.org/ontobpr#BuildingCodeDictionary"/>
        <rdfs:comment>BuildingAuthority checks a building regarding a BuildingCodeDictionary</rdfs:comment>
        <rdfs:label>checks</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#hasApplicationDocument -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#hasApplicationDocument">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ontobpr#hasDocument"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#BuildingApplication"/>
        <rdfs:range rdf:resource="https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/ontobpr#hasBuildingDocument"/>
        <rdfs:comment>a BuildingApplication has a respective document (e.g. PDF) representing a human-readable version of the application</rdfs:comment>
        <rdfs:label>has application document</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#hasBuildingApplicationContainer -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#hasBuildingApplicationContainer">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#BuildingApplication"/>
        <rdfs:range rdf:resource="https://standards.iso.org/iso/21597/-1/ed-1/en/Container#ContainerDescription"/>
        <rdfs:comment>the reference from a BuildingApplication to an ICDD container</rdfs:comment>
        <rdfs:label>has building application container</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#hasBuildingDocument -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#hasBuildingDocument">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ontobpr#hasDocument"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#Building"/>
        <rdfs:range rdf:resource="https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document"/>
        <rdfs:comment>A Building has a document, which is the IFC representation of this Building</rdfs:comment>
        <rdfs:label>has building document</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#hasCheckingResults -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#hasCheckingResults">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#BuildingApplication"/>
        <rdfs:range rdf:resource="https://w3id.org/ontobpr#ReviewResult"/>
        <rdfs:comment>either a BuildingApplication has ReviewResult or a ReviewResult has sh:ValidationReport</rdfs:comment>
        <rdfs:label>has checking results</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#hasCurrentActivity -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#hasCurrentActivity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#BuildingApplication"/>
        <rdfs:range rdf:resource="https://w3id.org/ontobpr#Activity"/>
        <rdfs:comment>a BuildingApplication is in the current Activity</rdfs:comment>
        <rdfs:label>has current activity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#hasDocument -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#hasDocument">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#BuildingApplication"/>
        <rdfs:range rdf:resource="https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document"/>
        <rdfs:comment>abstract property grouping hasApplicationDocument and hasBuildingDocument</rdfs:comment>
        <rdfs:label>has document</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#hasDocumentClassification -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#hasDocumentClassification">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document"/>
        <rdfs:range rdf:resource="https://w3id.org/ontobpr#DocumentClassification"/>
        <rdfs:comment>The classification of a ct:Document in an ICDD container in the context of a building application.</rdfs:comment>
        <rdfs:label>has document classification</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#hasShapes -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#hasShapes">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#ShaclShapesSet"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/shacl#Shape"/>
        <rdfs:comment>ShaclShapesSet has sh:Shape instances</rdfs:comment>
        <rdfs:label>has shapes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#hasShapesSet -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#hasShapesSet">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#BuildingCodeDictionary"/>
        <rdfs:range rdf:resource="https://w3id.org/ontobpr#ShaclShapesSet"/>
        <rdfs:comment>either an Activity or a BuildingCodeDictionary has a ShaclShapesSet</rdfs:comment>
        <rdfs:label>has shapes set</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#hasStatement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#hasStatement">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#BuildingApplication"/>
        <rdfs:range rdf:resource="https://w3id.org/ontobpr#ReviewStatement"/>
        <rdfs:comment>any ReviewStatement of any reviewer is attached to a BuildingApplication</rdfs:comment>
        <rdfs:label>has statement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontobpr#hasStatus -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontobpr#hasStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontobpr#BuildingApplication"/>
        <rdfs:range rdf:resource="https://w3id.org/ontobpr#ReviewStatus"/>
        <rdfs:comment>the ReviewStatus of a BuildingApplication</rdfs:comment>
        <rdfs:label>has status</rdfs:label>
    </owl:ObjectProperty>
    


