        <owl:equivalentClass rdf:resource="https://w3id.org/ontobpr#Activity"/>
    </owl:Class>
    


    <!-- https://w3id.org/obpa#Building -->


    <owl:Class rdf:about="https://w3id.org/obpa#Building">
        <owl:equivalentClass rdf:resource="https://w3id.org/ontobpr#Building"/>
    </owl:Class>
    


    <!-- https://w3id.org/obpa#BuildingApplication -->


    <owl:Class rdf:about="https://w3id.org/obpa#BuildingApplication">
        <owl:equivalentClass rdf:resource="https://w3id.org/ontobpr#BuildingApplication"/>
    </owl:Class>
    


    <!-- https://w3id.org/obpa#BuildingAuthority -->


    <owl:Class rdf:about="https://w3id.org/obpa#BuildingAuthority">
        <owl:equivalentClass rdf:resource="https://w3id.org/ontobpr#BuildingAuthority"/>
    </owl:Class>
    


    <!-- https://w3id.org/ontobpr#Activity -->


    <owl:Class rdf:about="https://w3id.org/ontobpr#Activity">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ontobpr#isCompleted"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>an Activity executed throughout the building permit review</rdfs:comment>
        <rdfs:label>Activity</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontobpr#Assignment -->


    <owl:Class rdf:about="https://w3id.org/ontobpr#Assignment">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontobpr#Activity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ontobpr#afterActivity"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/ontobpr#FormalReview"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>the Assignment activity</rdfs:comment>
        <rdfs:label>(2) Assignment</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontobpr#Building -->


    <owl:Class rdf:about="https://w3id.org/ontobpr#Building">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ontobpr#hasBuildingDocument"/>
                <owl:someValuesFrom rdf:resource="https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The building that is subject of this building application</rdfs:comment>
        <rdfs:label>Building</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontobpr#BuildingApplication -->


    <owl:Class rdf:about="https://w3id.org/ontobpr#BuildingApplication">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ontobpr#hasApplicationDocument"/>
