    <!-- https://w3id.org/isoprops#Dictionary -->


    <owl:Class rdf:about="https://w3id.org/isoprops#Dictionary">
        <owl:equivalentClass rdf:resource="https://w3id.org/ontobpr#BuildingCodeDictionary"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ontobpr#hasShapesSet"/>
