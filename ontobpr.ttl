###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/ontobpr#afterActivity
:afterActivity rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdf:type owl:TransitiveProperty ;
               rdfs:domain :Activity ;
               rdfs:range :Activity ;
               rdfs:comment "an activity is executed after another activity" ;
               rdfs:label "after activity" .


###  https://w3id.org/ontobpr#applies
:applies rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain <https://w3id.org/obpa#BuildingApplication> ,
                     :BuildingApplication ;
         rdfs:range <https://w3id.org/isoprops#Dictionary> ,
                    :BuildingCodeDictionary ;
         rdfs:comment "BuildingApplication applies a BuildingCodeDictionary for checking" ;
         rdfs:label "applies" .


###  https://w3id.org/ontobpr#belongsToActivity
:belongsToActivity rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain :ReviewStatement ;
                   rdfs:range :Activity ;
                   rdfs:comment "a ReviewStatement belongs to a particular Activity" ;
                   rdfs:label "belongs to activity" .


###  https://w3id.org/ontobpr#checks
:checks rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:domain <https://w3id.org/obpa#BuildingAuthority> ,
                    :BuildingAuthority ;
        rdfs:range <https://w3id.org/isoprops#Dictionary> ,
                   :BuildingCodeDictionary ;
        rdfs:comment "BuildingAuthority checks a building regarding a BuildingCodeDictionary" ;
        rdfs:label "checks" .


###  https://w3id.org/ontobpr#hasApplicationDocument
:hasApplicationDocument rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :hasDocument ;
                        rdfs:domain :BuildingApplication ;
                        rdfs:range <https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document> ;
                        owl:propertyDisjointWith :hasBuildingDocument ;
                        rdfs:comment "a BuildingApplication has a respective document (e.g. PDF) representing a human-readable version of the application" ;
                        rdfs:label "has application document" .


###  https://w3id.org/ontobpr#hasBuildingApplicationContainer
:hasBuildingApplicationContainer rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf owl:topObjectProperty ;
                                 rdfs:domain :BuildingApplication ;
                                 rdfs:range <https://standards.iso.org/iso/21597/-1/ed-1/en/Container#ContainerDescription> ;
                                 rdfs:comment "the reference from a BuildingApplication to an ICDD container" ;
                                 rdfs:label "has building application container" .


###  https://w3id.org/ontobpr#hasBuildingDocument
:hasBuildingDocument rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasDocument ;
                     rdfs:domain :Building ;
                     rdfs:range <https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document> ;
                     rdfs:comment "A Building has a document, which is the IFC representation of this Building" ;
                     rdfs:label "has building document" .


###  https://w3id.org/ontobpr#hasCheckingResults
:hasCheckingResults rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain :BuildingApplication ;
                    rdfs:range :ReviewResult ;
                    rdfs:comment "either a BuildingApplication has ReviewResult or a ReviewResult has sh:ValidationReport" ;
                    rdfs:label "has checking results" .


###  https://w3id.org/ontobpr#hasCurrentActivity
:hasCurrentActivity rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain :BuildingApplication ;
                    rdfs:range :Activity ;
                    rdfs:comment "a BuildingApplication is in the current Activity" ;
                    rdfs:label "has current activity" .


###  https://w3id.org/ontobpr#hasDocument
:hasDocument rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :BuildingApplication ;
             rdfs:range <https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document> ;
             rdfs:comment "abstract property grouping hasApplicationDocument and hasBuildingDocument" ;
             rdfs:label "has document" .


###  https://w3id.org/ontobpr#hasDocumentClassification
:hasDocumentClassification rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           rdfs:domain <https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document> ;
                           rdfs:range :DocumentClassification ;
                           rdfs:comment "The classification of a ct:Document in an ICDD container in the context of a building application." ;
                           rdfs:label "has document classification" .


###  https://w3id.org/ontobpr#hasShapes
:hasShapes rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :ShaclShapesSet ;
           rdfs:range <http://www.w3.org/ns/shacl#Shape> ;
           rdfs:comment "ShaclShapesSet has sh:Shape instances" ;
           rdfs:label "has shapes" .


###  https://w3id.org/ontobpr#hasShapesSet
:hasShapesSet rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :BuildingCodeDictionary ;
              rdfs:range :ShaclShapesSet ;
              rdfs:comment "either an Activity or a BuildingCodeDictionary has a ShaclShapesSet" ;
              rdfs:label "has shapes set" .


###  https://w3id.org/ontobpr#hasStatement
:hasStatement rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :BuildingApplication ;
              rdfs:range :ReviewStatement ;
              rdfs:comment "any ReviewStatement of any reviewer is attached to a BuildingApplication" ;
              rdfs:label "has statement" .


###  https://w3id.org/ontobpr#hasStatus
:hasStatus rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :BuildingApplication ;
           rdfs:range :ReviewStatus ;
           rdfs:comment "the ReviewStatus of a BuildingApplication" ;
           rdfs:label "has status" .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/ontobpr#isCompleted
:isCompleted rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Activity ;
             rdfs:range xsd:boolean ;
             rdfs:comment "a boolean property indicating whether an activity is completet or not" ;
             rdfs:label "is completed" .


#################################################################
#    Classes
#################################################################

###  http://org.semanticweb.owlapi/error#Error27
<http://org.semanticweb.owlapi/error#Error27> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error28
<http://org.semanticweb.owlapi/error#Error28> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error29
<http://org.semanticweb.owlapi/error#Error29> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error30
<http://org.semanticweb.owlapi/error#Error30> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error31
<http://org.semanticweb.owlapi/error#Error31> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error32
<http://org.semanticweb.owlapi/error#Error32> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error33
<http://org.semanticweb.owlapi/error#Error33> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error34
<http://org.semanticweb.owlapi/error#Error34> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error35
<http://org.semanticweb.owlapi/error#Error35> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error36
<http://org.semanticweb.owlapi/error#Error36> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error37
<http://org.semanticweb.owlapi/error#Error37> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error38
<http://org.semanticweb.owlapi/error#Error38> rdf:type owl:Class .


###  http://org.semanticweb.owlapi/error#Error39
<http://org.semanticweb.owlapi/error#Error39> rdf:type owl:Class .


###  http://www.w3.org/ns/shacl#Shape
<http://www.w3.org/ns/shacl#Shape> rdf:type owl:Class .


###  http://www.w3.org/ns/shacl#ValidationReport
<http://www.w3.org/ns/shacl#ValidationReport> rdf:type owl:Class .


###  https://schema.org/Person
<https://schema.org/Person> rdf:type owl:Class .


###  https://standards.iso.org/iso/21597/-1/ed-1/en/Container#ContainerDescription
<https://standards.iso.org/iso/21597/-1/ed-1/en/Container#ContainerDescription> rdf:type owl:Class .


###  https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document
<https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document> rdf:type owl:Class .


###  https://w3id.org/isoprops#Dictionary
<https://w3id.org/isoprops#Dictionary> rdf:type owl:Class ;
                                       owl:equivalentClass :BuildingCodeDictionary ;
                                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                         owl:onProperty :hasShapesSet ;
                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass :ShaclShapesSet
                                                       ] .


###  https://w3id.org/obpa#Activity
<https://w3id.org/obpa#Activity> rdf:type owl:Class ;
                                 owl:equivalentClass :Activity .


###  https://w3id.org/obpa#Building
<https://w3id.org/obpa#Building> rdf:type owl:Class ;
                                 owl:equivalentClass :Building .


###  https://w3id.org/obpa#BuildingApplication
<https://w3id.org/obpa#BuildingApplication> rdf:type owl:Class ;
                                            owl:equivalentClass :BuildingApplication .


###  https://w3id.org/obpa#BuildingAuthority
<https://w3id.org/obpa#BuildingAuthority> rdf:type owl:Class ;
                                          owl:equivalentClass :BuildingAuthority .


###  https://w3id.org/ontobpr#Activity
:Activity rdf:type owl:Class ;
          rdfs:subClassOf owl:Thing ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :isCompleted ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:boolean
                          ] ;
          rdfs:comment "an Activity executed throughout the building permit review" ;
          rdfs:label "Activity" .


###  https://w3id.org/ontobpr#Assignment
:Assignment rdf:type owl:Class ;
            rdfs:subClassOf :Activity ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :afterActivity ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :FormalReview
                            ] ;
            rdfs:comment "the Assignment activity" ;
            rdfs:label "(2) Assignment" .


###  https://w3id.org/ontobpr#Building
:Building rdf:type owl:Class ;
          rdfs:subClassOf owl:Thing ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasBuildingDocument ;
                            owl:someValuesFrom <https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document>
                          ] ;
          rdfs:comment "The building that is subject of this building application" ;
          rdfs:label "Building" .


###  https://w3id.org/ontobpr#BuildingApplication
:BuildingApplication rdf:type owl:Class ;
                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasApplicationDocument ;
                                       owl:someValuesFrom <https://standards.iso.org/iso/21597/-1/ed-1/en/Container#Document>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasCheckingResults ;
                                       owl:someValuesFrom :ReviewResult
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasStatement ;
                                       owl:someValuesFrom :ReviewStatement
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasBuildingApplicationContainer ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass <https://standards.iso.org/iso/21597/-1/ed-1/en/Container#ContainerDescription>
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasCurrentActivity ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :Activity
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :hasStatus ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :ReviewStatus
                                     ] ;
                     rdfs:comment "the building application that is submitted by an applicant for review" ;
                     rdfs:isDefinedBy <https://w3id.org/obpa#BuildingApplication> ;
                     rdfs:label "Building application" .


###  https://w3id.org/ontobpr#BuildingAuthority
:BuildingAuthority rdf:type owl:Class ;
                   rdfs:comment "a building authority that is involved in a building permit review" ;
                   rdfs:isDefinedBy <https://w3id.org/obpa#BuildingAuthority> ;
                   rdfs:label "Building authority" .


###  https://w3id.org/ontobpr#BuildingCodeDictionary
:BuildingCodeDictionary rdf:type owl:Class ;
                        rdfs:subClassOf [ rdf:type owl:Restriction ;
                                          owl:onProperty :hasShapesSet ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass :ShaclShapesSet
                                        ] ;
                        rdfs:comment "A dictionary containing properties and shapes from a building code" ;
                        rdfs:label "Dictionary" .


###  https://w3id.org/ontobpr#CompletenessCheck
:CompletenessCheck rdf:type owl:Class ;
                   rdfs:subClassOf :Activity ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasShapesSet ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :ShaclShapesSet
                                   ] ;
                   rdfs:comment "the Completeness check activity" ;
                   rdfs:label "(1.1) Completeness check " .


###  https://w3id.org/ontobpr#Condition
:Condition rdf:type owl:Class ;
           rdfs:subClassOf :ReviewStatement ;
           owl:disjointWith :Justification ;
           rdfs:comment "a formulated condition statement of a conditionally accepted application" ;
           rdfs:label "Condition" .


###  https://w3id.org/ontobpr#ContentReview
:ContentReview rdf:type owl:Class ;
               rdfs:subClassOf :Activity ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :afterActivity ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :FormalReview
                               ] ;
               rdfs:comment "The Content review activity" ;
               rdfs:label "(4) Content review " .


###  https://w3id.org/ontobpr#CreatingNotificationLetter
:CreatingNotificationLetter rdf:type owl:Class ;
                            rdfs:subClassOf :Activity ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :afterActivity ;
                                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass :FormulationOfCondition
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :afterActivity ;
                                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass :JustificationOfNegativeDecision
                                            ] ;
                            rdfs:comment "The Creating notification letter activity" ;
                            rdfs:label "(5.4) Creating notification letter" .


###  https://w3id.org/ontobpr#DocumentClassification
:DocumentClassification rdf:type owl:Class ;
                        rdfs:subClassOf owl:Thing ;
                        rdfs:comment "The classification of a document in the context of a building application." ;
                        rdfs:label "Document classification" .


###  https://w3id.org/ontobpr#FormalReview
:FormalReview rdf:type owl:Class ;
              rdfs:subClassOf <http://org.semanticweb.owlapi/error#Error29> ,
                              <http://org.semanticweb.owlapi/error#Error30> ,
                              :Activity ;
              rdfs:comment "The Formal review activity" ;
              rdfs:label "(1) Formal review" .


###  https://w3id.org/ontobpr#FormulationOfCondition
:FormulationOfCondition rdf:type owl:Class ;
                        rdfs:subClassOf :Activity ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :afterActivity ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass :PositivePermitDecision
                                        ] ;
                        rdfs:comment "the Formulation of condition activity" ;
                        rdfs:label "(5.3a) Formulation of condition " .


###  https://w3id.org/ontobpr#IssuingNotificationLetter
:IssuingNotificationLetter rdf:type owl:Class ;
                           rdfs:subClassOf <http://org.semanticweb.owlapi/error#Error31> ,
                                           <http://org.semanticweb.owlapi/error#Error32> ,
                                           <http://org.semanticweb.owlapi/error#Error33> ,
                                           <http://org.semanticweb.owlapi/error#Error34> ,
                                           <http://org.semanticweb.owlapi/error#Error35> ,
                                           <http://org.semanticweb.owlapi/error#Error36> ,
                                           :Activity ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :afterActivity ;
                                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass :Assignment
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :afterActivity ;
                                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass :ContentReview
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :afterActivity ;
                                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass :Participation
                                           ] ;
                           rdfs:comment "the Issuing notification letter activity" ;
                           rdfs:label "(5) Issuing notification letter " .


###  https://w3id.org/ontobpr#Justification
:Justification rdf:type owl:Class ;
               rdfs:subClassOf :ReviewStatement ;
               rdfs:comment "justification statement of a negative building permit review decision" ;
               rdfs:label "Justification" .


###  https://w3id.org/ontobpr#JustificationOfNegativeDecision
:JustificationOfNegativeDecision rdf:type owl:Class ;
                                 rdfs:subClassOf :Activity ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :afterActivity ;
                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass :NegativePermitDecision
                                                 ] ;
                                 rdfs:comment "the Justification of negative decision activity" ;
                                 rdfs:label "(5.3b) Justification of negative decision " .


###  https://w3id.org/ontobpr#NegativePermitDecision
:NegativePermitDecision rdf:type owl:Class ;
                        rdfs:subClassOf :Activity ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :afterActivity ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass :RequestReviewResults
                                        ] ;
                        rdfs:comment "the Negative permit decision activity" ;
                        rdfs:label "(5.2b) Negative permit decision " .


###  https://w3id.org/ontobpr#Participation
:Participation rdf:type owl:Class ;
               rdfs:subClassOf :Activity ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :afterActivity ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :FormalReview
                               ] ;
